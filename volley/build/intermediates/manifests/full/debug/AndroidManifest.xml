<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="moon.volley"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.INTERNET" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.1" />

    <application
        android:name="moon.volley.MApplication.MApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="moon.volley.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="moon.volley.GetRequestActivity"
            android:label="get请求"
            android:screenOrientation="portrait" />
        <activity
            android:name="moon.volley.PostRequestActivity"
            android:label="post请求"
            android:screenOrientation="portrait" />
        <activity
            android:name="moon.volley.PostFormActivity"
            android:label="post表单提交"
            android:screenOrientation="portrait" />
        <activity
            android:name="moon.volley.PostUploadActivity"
            android:label="post上传图片"
            android:screenOrientation="portrait" />
    </application>

</manifest>